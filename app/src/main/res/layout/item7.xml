<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal"
    tools:showIn="@layout/content_lar_tab7_list">

    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:id="@+id/column1content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/t07_id"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="日期"
            android:background="@color/gray"
            android:textColor="@color/white"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/normal_bg_color" />

    </LinearLayout>

    <com.gy.myframework.UI.customwidget.mutihostlist.InterceptScrollContainer
        android:id="@+id/scroollContainter"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@id/column1content"
        android:focusable="false" >

        <com.gy.myframework.UI.customwidget.mutihostlist.MyHScrollView
            android:id="@+id/horizontalScrollView1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:scrollbars="none" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:orientation="horizontal" >


                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_sumtime"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="总运行时"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_overup_time"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="超上限时"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_overup_per"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="超上限率"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_overdown_time"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="超下限时"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_overdown_per"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="超下限率"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>

                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />

                <LinearLayout
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_passper"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:text="合格率"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />
                <LinearLayout
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_max"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:text="最大值"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />
                <LinearLayout
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_max_time"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:text="最大值时"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />
                <LinearLayout
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_min"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:text="最小值"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />
                <LinearLayout
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_min_time"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:text="最小值时"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/normal_bg_color" />
                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/t07_avr"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="平均值"
                        android:singleLine="true"
                        android:textAppearance="?android:attr/textAppearanceMedium" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/normal_bg_color" />
                </LinearLayout>

            </LinearLayout>

        </com.gy.myframework.UI.customwidget.mutihostlist.MyHScrollView>
    </com.gy.myframework.UI.customwidget.mutihostlist.InterceptScrollContainer>

</RelativeLayout>